    <resources>
        <j2se version="1.5* 1.6*" initial-heap-size="100m" max-heap-size="256m" java-vm-args="-XX:+DisableExplicitGC"/>
    </resources>
    <resources>
        <j2se version="1.5+"/>
        <jar href="release/JViewer.jar"/>
    </resources>
    <resources os="Windows" arch="x86">
